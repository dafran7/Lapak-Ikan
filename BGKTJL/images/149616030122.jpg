extern printf
extern fflush
extern scanf
   
section .data
    fmt db  "%d", 0
    fmt2 db  "%d",10 ,0
       
section .bss
    a resd 1

 
section .text
    global main
       
main :
    ;read
    push a
    push fmt
    call scanf
    add esp, 20
	mov ecx,[a]
	mov ebx,2
	mov eax,1

       
	;loop
	meong:
		mul ebx
		loop meong
	
     
    push eax
    push fmt2
    call printf
    add esp, 4 
    push 0
    call fflush
    jmp exit
 
 
exit :
    mov eax,1
    mov ebx,0
    int 0x80
